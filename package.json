  },
  "license": "MIT",
  "devDependencies": {
    "@types/mocha": "^9.1.1",
    "@types/node": "^16.10.2",
    "mocha": "^9.1.2",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.4"
  }
}
